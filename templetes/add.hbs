 </div>
<form method='POST' className="register-form" id="register-form">
            
            
                  <div class="form-group">
                    <label htmlFor="email">
                      <i class="zmdi zmdi-email material-icons-name"></i>
                    </label>
                    <input type="email" name="email" id="email" autoComplete="off"
                  
                      placeholder="Employee Email" />
                  </div>


                  <div class="form-group">
                    <label htmlFor="Name">
                      <i class="zmdi zmdi-account material-icons-name"></i>
                    </label>
                    <input type="text" name="ename" id="name" autoComplete="off"
                   
                      placeholder="Employee Name" />
                  </div>


                  <div class="form-group">
                    <label htmlFor="phone">
                      <i class="zmdi zmdi-phone-in-talk material-icons-name"></i>
                    </label>
                    <input type="text" name="pnumber" id="phone" autoComplete="off"
                      
                      placeholder="Employee Phone Number" />
                  </div>

                  <div class="form-group">
                    <label htmlFor="work">
                      <i className="zmdi zmdi-slideshow material-icons-name"></i>
                    </label>
                    <input type="text" name="salary" id="work" autoComplete="off"
                      placeholder="Employee Salary" />
                  </div>
                  
                  <div className="form-group">
                    <label htmlFor="Processed">
                      <i className="zmdi zmdi-slideshow material-icons-name"></i>
                    </label>
                    <input type="text" name="processed" id="Degree" autoComplete="off"
                      placeholder="Salary Processed or not" />
                  </div>
                  
                  
                  <div class="form-group">
                    <label htmlFor="work">
                      <i className="zmdi zmdi-slideshow material-icons-name"></i>
                    </label>
                    <input type="text" name="leaves" id="work" autoComplete="off"
                      placeholder="Number of leaves" />
                  </div>
                 
                  <div class="form-group">
                    <label htmlFor="Comments">
                      <i class="zmdi zmdi-slideshow material-icons-name"></i>
                    </label>
                    <input type="text" name="comments" id="College" autoComplete="off"
                      placeholder="Any Comments" />
                  </div>

                  <div class="form-group form-button">
                    <input type="submit" name="signup" id="signup" class="form_submit" value="Add Employee"/>

                  </div>


                </form>
                




</body>
</html>