 </div>
    <div class="emp">
     <h1 class="headin">Here is the list of company Employs</h1>
        {{#each result}}
      <div class="empi">
        <ul>    
       <li class="edata" > <b> Company ID </b>  <br>{{this._id}}</li>
       <li class="edata" > <b> Employee Name</b> <br> {{this.ename}}</li>
       
       <li class="edata" ><b> Employee Email ID</b> <br>{{this.email}}</li>
       <li class="edata" > <b> Employee Phone Number </b> <br>{{this.pnumber}}</li>
       
       <li class="edata" > <b> Package </b> <br>{{this.salary}} </li>
       <li class="edata" > <b> Salary Processed </b><br>{{this.processed}}</li>

       
       <li class="edata" > <b> No of leaves in this month</b><br> {{this.leaves}}</li>
       <li class="edata" > <b> Comments </b> <br>{{this.comments}}</li>
  </ul>
       </div>
        {{/each}}
    
    
    </div>
